    "#Norm\n",
    "p = [1,2,3,'infinity']\n",
    "v = [1, -1, 2, 0]\n",
    "nrm = 0\n",
    "\n",
    "for i in range(len(p)):\n",
    "    if p[i] == 'infinity':\n",
    "        print(\"norm\",p[i],\"=\", max(v))\n",
    "    else:\n",
    "        for j in range(len(v)):\n",
    "            nrm = nrm + abs(v[j])**p[i]\n",
    "        print(\"norm\",p[i],\"=\",nrm)\n",
    "    \n",
    "        "
